{
  "name": "Workabyte Go",
  "description": "Modular keyboard launcher for web workflows. Built by Workabyte LLC.",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "omnibox": {
    "keyword": "go"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  }
}